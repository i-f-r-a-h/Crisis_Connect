 (err) {\n          setError(err.message);\n          setData(null);\n        } finally {\n          setLoading(false);\n        }\n      };\n      getData();\n    }, []);\n    return (\n        <Fragment>\n            <section className=\"map\">\n   <div className=\"options__wrapper\">\n\n\n<div className=\"options\">\n    <div className=\"options__panel\">\n            <button className=\"options__panel__all\">All</button>\n            <button className=\"options__panel__ongoing\">Ongoing</button>\n            <button className=\"options__panel__alerts\">Alerts</button>\n    </div>\n\n    <div className=\"options__info\">\n    <h2 className=\"options__info__title\">\n        Filter by Country\n      </h2>\n      <Box\n      sx={{ width: '100%', height: '100vh' }}\n    >\n      <FixedSizeList\n        height={800}\n        width={360}\n        itemSize={46}\n        itemCount={200}\n        overscanCount={5}\n      >\n        {renderRow}\n      </FixedSizeList>\n    </Box>\n      {/* {loading && <div>A moment please...</div>}\n      {error && (\n        <div>{`There is a problem fetching the post data - ${error}`}</div>\n      )}\n      <ul>\n        {data &&\n          data.data.map(({ id, fields }) => (\n            <li key={id}>\n              <h3>{fields.name}</h3>\n              <a href=\"{href}\">Link to article</a>\n            </li>\n          ))}\n      </ul> */}\n    </div>\n</div>\n<div className=\"options__toggle\"><Fab size=\"small\"  aria-label=\"close\">\n  <ArrowBackIosNewRoundedIcon />\n</Fab></div>\n</div>\n\n\n                \n            </section>\n            \n        </Fragment>\n    )\n\n}\n\nexport default InteractiveMap;","/Users/ifrahvermeer/Desktop/group-project/crisis-connect/client/src/routes/community/community.component.jsx",["147","148","149"],[],"import { Fragment } from \"react\";\nimport {Outlet, Link, useLocation} from 'react-router-dom';\nimport Box from '@mui/material/Box';\nimport TextField from '@mui/material/TextField';\n