        {\n          \"id\": 3,\n          \"title\": \"Facebook\",\n          \"imageUrl\": \"https://i.ibb.co/0jqHpnp/sneakers.png\"\n        }\n      \n    ]\n\n  return (\n    <footer class='footer'>\n      \n      <Marquee socials={socials}/>\n      <section className='footer__main'>\n            <div className='footer__tagline'>\n             <p>Join the community</p>\n             </div>\n        <div className='footer__container'>\n            <div className='footer__newsletter'>\n                  <p>Join Our Subscription List</p>\n                  <input type={'email'} placeholder=\"Enter email\"/>\n                  <p class=\"footer__newsletter__info\">Join our newsletter to stay up to date on features and releases</p>\n            </div>\n          \n          <div className='footer__links'>\n            <nav className='footer__links__pages'>\n              <p>Pages</p>\n              <ul>\n                  <li>\n                    <a href=\"http://localhost:3000/\" target=\"_blank\" rel=\"noreferrer\">Your Profile</a>\n                  </li>\n                  <li>\n                    <a href=\"http://localhost:3000/\" target=\"_blank\" rel=\"noreferrer\">About Us</a>\n                  </li>\n                  <li>\n                    <Link className='community' to={'/Community'}> Community Hub </Link>\n                  </li>\n                  <li>\n                    <Link className='contact' to={'/Contact'}> Contact </Link>\n                  </li>\n                </ul>\n            </nav>\n\n            <nav className='footer__links__external'>\n              <p>Links</p>\n              <ul>\n                <li>\n                    <a href=\"http://localhost:3000/\" target=\"_blank\" rel=\"noreferrer\">Terms & conditions</a>\n                </li>\n                  <li>\n                    <a href=\"http://localhost:3000/\" target=\"_blank\" rel=\"noreferrer\">Privacy Policy</a>\n                </li>\n                  <li>\n                    <a href=\"http://localhost:3000/\" target=\"_blank\" rel=\"noreferrer\">Site Credit</a>\n                  </li>\n                </ul>\n            </nav>\n          </div>\n       </div>\n       {/* <img src={Bamboo} alt=\"footer bamboo\" className='footer__bamboo' /> */}\n\n      </section>\n     \n      \n    </footer>\n  )\n}\n\nexport default Footer\n","/Users/ifrahvermeer/Desktop/group-project/crisis-connect/client/src/components/button/button.component.jsx",[],[],"/Users/ifrahvermeer/Desktop/group-project/crisis-connect/client/src/routes/authmodal/AuthModal.jsx",[],[],"/Users/ifrahvermeer/Desktop/group-project/crisis-connect/client/src/routes/contact/contact.jsx",["121"],[],"import { Link } from 'react-router-dom';\n\n\nconst Contact = () => {\n    return(\n        <main>\n            <h1>We want to hear from you.</h1>\n            <p>We are available for your comments, requests and assistance.\n                Please use the form below to contact us.\n            </p>\n        </main>\n\n    )\n}\nexport default Contact;","/Users/ifrahvermeer/Desktop/group-project/crisis-connect/client/src/components/home/Hero.component.jsx",["122"],[],"import { Fragment } from \"react\";\nimport Button from \"../button/button.component\";\n\nconst Hero = () => {\n    return(\n        <Fragment>\n        <section className=\"hero\">\n            <div className=\"hero__container\">\n                   <h1>Be the Change with Crisis Connect</h1>\n                   <p>Humanitarian crises are affecting millions of people worldwide. It's time for us to come together and raise awareness about the pressing issues happening around the world.</p>\n                   <div className=\"hero__buttons\">\n                   <button className='signup' to={'/InteractiveMap'}>Start Exploring</button>\n                   <button className='signup' to={'/Community'}>Join the community</button>\n                   </div>\n            </div>\n        </section>\n        <section className=\"hero__banner\">\n            <div className=\"Hero__image\"></div>\n            <div className=\"Hero__card\"></div>\n        </section>\n        </Fragment>\n    )\n}\n\nexport default Hero;","/Users/ifrahvermeer/Desktop/group-project/crisis-connect/client/src/components/home/Faqs.component.jsx",["123","124"],[],"import { useState } from \"react\";\nimport { accordionData } from \"../../utils/content/accordionData\";\n\nconst Accordion = ({ title, content }) => {\n    const [isActive, setIsActive] = useState(false);\n  \n    return (\n      <div className=\"accordion__item\">\n        <div className=\"accordion__title\" onClick={() => setIsActive(!isActive)}>\n          <div>{title}</div>\n          <div className=\"accordion__dropdown\">{isActive ? '-' : '+'}</div>\n        </div>\n        {isActive && <div className=\"accordion__content\">{content}</div>}\n      </div>\n    );\n};\n\nconst Faqs = () => {\n    const [isActive, setIsActive] = useState(false);\n\n    return (\n     <section className='faqs'>\n        <div>\n            <h2 className='faqs__heading'>FAQs</h2>\n            <p className='faqs__subheading'>Helping you <br/>to help others</p>\n        </div>\n        <div className=\"faqs__accordion\">\n        {accordionData.map(({ title, content }) => (\n          <Accordion title={title} content={content} />\n        ))}\n      </div>\n        \n    </section>\n    )\n\n}\n\n\nexport default Faqs;","/Users/ifrahvermeer/Desktop/group-project/crisis-connect/client/src/components/home/Testimonials.component.jsx",[],[],"/Users/ifrahvermeer/Desktop/group-project/crisis-connect/client/src/components/home/Discover.component.jsx",["125"],[],"import girl from '../../assets/girl.png'\nimport cloud from '../../assets/cloud.png'\nimport Button from '../button/button.component';\n\n\nconst Discover = () => {\n    return(\n        <section className=\"discover\">\n            <div className=\"discover__title\">\n                <h2>Discover The <br/> Ultimate Destination <br/> For Global Awareness</h2>\n                <p>We are so glad you're here! Our mission is to bring awareness to crisis situations and provide a platform for those in need to connect with the help they require.</p>\n            </div>\n            <div className=\"discover__cards\">\n                <div className=\"discover__cards__top\">\n                    <div className='discover__cards__container'>\n                        <h3>Unite Against Crises</h3>\n                        <p>With Crisis Connect, you can join the movement to raise awareness about humanitarian issues. Share your experiences and help bring attention to the cause. By working together, we can create a better world for all.</p>\n                         <img src={girl} alt=\"3d girl with glasses waving\"  /> \n                    </div>\n\n                    <div className='discover__cards__container'>\n                        <h3>Know more, Help more</h3>\n                        <p>Our interactive map powered by Three.js and WebGL keeps you updated with real-time information provided by the ReliefWeb API, you can stay informed about the latest developments.</p>\n                         <img src={cloud} alt=\"3d Sun cloud angled rain\"  /> \n                    </div>\n\n                </div>\n                <div className=\"discover__cards__bottom\">\n                    <div className='discover__cards__container'>\n                        <h3>Be a Part of the Solution with Our Community Hub</h3>\n                        <p>Our Community Hub is a platform for people to connect and help one another. Share your stories, post for help, and be a part of the solution. With an intuitive design, it's never been easier to make a difference.</p>\n                        <button></button>\n                         <img src={girl} alt=\"3d girl with glasses waving\"  /> \n                    </div>\n                </div>\n            </div>\n        </section>\n    )\n}\n\nexport default Discover;","/Users/ifrahvermeer/Desktop/group-project/crisis-connect/client/src/components/marquee/marquee.component.jsx",[],[],"/Users/ifrahvermeer/Desktop/group-project/crisis-connect/client/src/utils/content/accordionData.jsx",[],[],"/Users/ifrahvermeer/Desktop/group-project/crisis-connect/client/src/components/marquee/socialItem.component.jsx",["126"],[],"import Button from \"../button/button.component\";\n\nconst SocialItem = ({ social }) => {\n    const { hrefLink, title } = social;\n    return (\n        <div className=\"marquee__inner\" >\n            <Button buttonType='social'>{title}</Button>\n            <Button buttonType='follow'>Follow us @CrisisConnect </Button>\n        </div>\n    );\n  };\n\n  export default SocialItem;","/Users/ifrahvermeer/Desktop/group-project/crisis-connect/client/src/routes/authmodal/login.jsx",[],[],"/Users/ifrahvermeer/Desktop/group-project/crisis-connect/client/src/routes/authmodal/register.jsx",["127","128"],[],"import { Link } from 'react-router-dom';\nimport { useEffect, useState } from 'react';\n\n\nconst SignUp = () => {\n    const [username, setUsername] = useState('');\n    const [password, setPassword] = useState('');\n    async function register(ev) {\n      ev.preventDefault();\n      const response = await fetch('http://localhost:4000/Register', {\n        method: 'POST',\n        body: JSON.stringify({username,password}),\n        headers: {'Content-Type':'application/json'},\n      });\n      if (response.status === 200) {\n        alert('registration successful');\n      } else {\n        alert('registration failed');\n      }\n    }\n    return (\n      <form className=\"register\" onSubmit={register}>\n        <h1>Register</h1>\n        <input type=\"text\"\n               placeholder=\"username\"\n               value={username}\n               onChange={ev => setUsername(ev.target.value)}/>\n        <input type=\"password\"\n               placeholder=\"password\"\n               value={password}\n               onChange={ev => setPassword(ev.target.value)}/>\n        <button>Register</button>\n      </form>\n    );\n}\nexport default SignUp;","/Users/ifrahvermeer/Desktop/group-project/crisis-connect/client/src/routes/authmodal/UserContext.js",[],[],{"ruleId":"129","severity":1,"message":"130","line":1,"column":26,"nodeType":"131","messageId":"132","endLine":1,"endColumn":32},{"ruleId":"129","severity":1,"message":"133","line":1,"column":48,"nodeType":"131","messageId":"132","endLine":1,"endColumn":52},{"ruleId":"129","severity":1,"message":"134","line":6,"column":8,"nodeType":"131","messageId":"132","endLine":6,"endColumn":17},{"ruleId":"135","severity":1,"message":"136","line":65,"column":25,"nodeType":"137","endLine":65,"endColumn":45},{"ruleId":"129","severity":1,"message":"138","line":3,"column":8,"nodeType":"131","messageId":"132","endLine":3,"endColumn":14},{"ruleId":"129","severity":1,"message":"133","line":1,"column":10,"nodeType":"131","messageId":"132","endLine":1,"endColumn":14},{"ruleId":"129","severity":1,"message":"139","line":2,"column":8,"nodeType":"131","messageId":"132","endLine":2,"endColumn":14},{"ruleId":"129","severity":1,"message":"140","line":19,"column":12,"nodeType":"131","messageId":"132","endLine":19,"endColumn":20},{"ruleId":"129","severity":1,"message":"141","line":19,"column":22,"nodeType":"131","messageId":"132","endLine":19,"endColumn":33},{"ruleId":"129","severity":1,"message":"139","line":3,"column":8,"nodeType":"131","messageId":"132","endLine":3,"endColumn":14},{"ruleId":"129","severity":1,"message":"142","line":4,"column":13,"nodeType":"131","messageId":"132","endLine":4,"endColumn":21},{"ruleId":"129","severity":1,"message":"133","line":1,"column":10,"nodeType":"131","messageId":"132","endLine":1,"endColumn":14},{"ruleId":"129","severity":1,"message":"143","line":2,"column":10,"nodeType":"131","messageId":"132","endLine":2,"endColumn":19},"no-unused-vars","'Router' is defined but never used.","Identifier","unusedVar"